      "@libs/sqs-consumers": [
        "./libs/sqs-consumers/src"
      ],
      "@libs/sqs-consumers/*": [
        "./libs/sqs-consumers/src/*"
      ],
      "@libs/encryption": [
        "./libs/encryption/src"
      ],
      "@libs/encryption/*": [
        "./libs/encryption/src/*"
      ],
      "@apps/platform/*": [
        "./apps/platform/src/*"
      ]
    }
  }
}
